{
  "expo": {
    "name": "HemaConnect-Mobile",
    "slug": "HemaConnect-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Pictures/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/Pictures/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.HemaConnect.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Pictures/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/Pictures/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "78392e4e-af75-40e9-91a7-c6bc5830d842"
      }
    }
  }
}
